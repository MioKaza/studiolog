{
    "name": "studiolog-tauri",
    "version": "0.1.2",
    "description": "Essential in-app terminal and debug console for Tauri and React desktop applications",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./styles.css": "./dist/index.css"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "demo.png"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint src --ext .ts,.tsx",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:run": "vitest run",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "console",
        "debug",
        "devtools",
        "logging",
        "react",
        "terminal",
        "tauri"
    ],
    "peerDependencies": {
        "framer-motion": ">=10",
        "lucide-react": ">=0.200.0",
        "react": ">=18",
        "react-dom": ">=18"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.0.0",
        "@testing-library/react": "^14.0.0",
        "@types/node": "^25.0.9",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@vitejs/plugin-react": "^4.2.0",
        "eslint": "^8.50.0",
        "framer-motion": "^10.0.0",
        "jsdom": "^22.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0"
    },
    "author": "Mio Kaza",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/miokaza/studiolog.git"
    },
    "homepage": "https://github.com/miokaza/studiolog#readme",
    "bugs": {
        "url": "https://github.com/miokaza/studiolog/issues"
    }
}